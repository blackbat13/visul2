<button id="start-button" type="button" class="btn btn-primary"><i class="fa fa-play" aria-hidden="true"></i></button>
<button id="pause-button" type="button" class="btn btn-primary"><i class="fa fa-pause" aria-hidden="true"></i></button>
<button id="restart-button" type="button" class="btn btn-primary"><i class="fa fa-refresh" aria-hidden="true"></i></button>
<input id="time-range" class="form-range" type="range" min="0" max="100" step=".001" value="0"/>
